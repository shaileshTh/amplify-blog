{
  "version": 1,
  "paths": {
    "/posts": {
      "name": "/posts",
      "lambdaFunction": "getPostsFunction",
      "permissions": {
        "setting": "private",
        "auth": [
          "create",
          "read",
          "update",
          "delete"
        ]
      }
    },
    "/create-payment-intent": {
      "name": "/create-payment-intent",
      "lambdaFunction": "createPaymentIntent",
      "permissions": {
        "setting": "open"
      }
    }
  }
}